{
  "name": "rest-express",
  "version": "2.8.17",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "noip": "node ./server/noip.js",
    "version:bump": "npm version patch && tsx scripts/update-sw-version.ts",
    "version:bump:minor": "npm version minor && tsx scripts/update-sw-version.ts",
    "version:bump:major": "npm version major && tsx scripts/update-sw-version.ts",
    "deploy": "tsx scripts/update-version.js && npm run build && git add . && git commit -m \"Auto-deploy: Version update\" && git push origin main",
    "deploy:patch": "npm run version:bump && npm run build && git add . && git commit -m \"Release: Patch version update\" && git push origin main",
    "deploy:minor": "npm run version:bump:minor && npm run build && git add . && git commit -m \"Release: Minor version update\" && git push origin main"
  },
  "dependencies": {
